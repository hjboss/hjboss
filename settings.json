{
  "editor.fontFamily": "CascadiaCode-SemiBold,IBM Plex Mono SemiBold,圓體-繁",
  "editor.fontSize": 10,
  "editor.lineHeight": 10,
  "editor.fontWeight": "normal",
  "editor.fontLigatures": false,
  "editor.detectIndentation": true,
  "editor.wordWrap": "wordWrapColumn",
  "editor.renderWhitespace": "boundary",
  "editor.renderIndentGuides": true,
  "editor.renderControlCharacters": true,
  "editor.wordWrapColumn": 80,
  "editor.letterSpacing": 0,
  "editor.tabSize": 2,
  "editor.formatOnSave": false,
  "editor.rulers": [80, 80],
  "editor.quickSuggestions": {
    "comments": true,
    "strings": true,
    "other": true
  },
  "terminal.integrated.inheritEnv": false,
  "terminal.integrated.fontFamily": "monospace",
  "typescript.referencesCodeLens.enabled": false,
  "typescript.implementationsCodeLens.enabled": false,
  "python.insidersChannel": "weekly",
  "python.pythonPath": "/usr/local/Homebrew/miniconda3/bin/python3",
  "python.linting.pycodestyleEnabled": false,
  "python.linting.pylintEnabled": true,
  "[python]": {
    "editor.tabSize": 2
  },
  "[rust]": {
    "editor.tabSize": 4
  },
  "[html]": {
    "editor.wordWrapColumn": 120,
    "editor.rulers": [120, 120],
  },
  "[markdown]": {
    "editor.fontFamily": "IBM Plex Mono Thin,圓體-繁",
    "editor.fontWeight": "bold",
    "editor.fontSize": 12,
    "editor.lineHeight": 12
  },
  "oneDarkPro.italic": false,
  "oneDarkPro.vivid": true,
  "oneDarkPro.markdownStyle": true,
  "oneDarkPro.editorTheme": "nord",
  "workbench.startupEditor": "welcomePage",
  "workbench.iconTheme": "vscode-icons",
  "workbench.colorCustomizations": {
    "[Solarized Light]": {
      "foreground": "#657B83",
      "activityBar.background": "#ECE7D5",
      "editor.foreground": "#657B83",
      "editor.background": "#FDF6E3",
      "editorGroupHeader.tabsBackground": "#FDF6E3",
      "editorIndentGuide.activeBackground": "#BDC3C7",
      "editorIndentGuide.background": "#ECE7D5",
      "sideBar.border": "#ECE7D5",
      "sideBar.background": "#FDF6E3",
      "sideBarSectionHeader.background": "#ECE7D5",
      "tab.border": "#FDF6E3",
      "tab.inactiveForeground": "#95A5A6",
      "tab.activeBorderTop": "#B58900",
      "tab.activeBackground": "#ECE7D5",
      "tab.activeForeground": "#657B83",
      "tab.inactiveBackground": "#FDF6E3",
      "terminal.foreground": "#95A5A6",
      "titleBar.activeBackground": "#FDF6E3",
      "titleBar.activeForeground": "#95A5A6"
    }
  },
  "editor.tokenColorCustomizations": {
    "[Solarized Light]": {
      "textMateRules": [{
        "scope": [
          "comment",
          "entity.other.attribute-name",
          "punctuation.definition.tag.begin",
          "punctuation.definition.tag.end"
        ],
        "settings": {
          "foreground": "#95A5A6",
          "fontStyle": ""
        }
      }, {
        "scope": [
          "keyword",
          "markup.quote",
          "punctuation.definition.variable",
          "support.class"
        ],
        "settings": {
          "foreground": "#d77f66",
          "fontStyle": ""
        }
      }, {
        "scope": [
          "entity.name.class",
          "entity.name.function",
          "entity.name.tag",
          "entity.other.inherited-class",
          "meta.function-call.generic",
          "entity.name.namespace",
          "markup.heading",
          "markup.heading.setext",
          "support.function",
          "variable.language",
          "variable.other"
        ],
        "settings": {
          "foreground": "#8ba3c7",
          "fontStyle": ""
        }
      }, {
        "scope": [
          "source.python support.variable",
          "support.type.primitive",
          "meta.preprocessor",
          "entity.name.type"
        ],
        "settings": {
          "foreground": "#c8b5b3",
          "fontStyle": ""
        }
      }, {
        "scope": [
          "punctuation.separator.continuation",
          "punctuation.section.embedded.begin",
          "punctuation.section.embedded.end",
          "support.function.construct",
          "string.regexp"
        ],
        "settings": {
          "foreground": "#bc6e37",
          "fontStyle": ""
        }
      }, {
        "scope": [
          "constant.numeric",
          "markup.changed",
          "markup.italic",
          "markup.bold",
          "markup.list"
        ],
        "settings": {
          "foreground": "#87c0ca",
          "fontStyle": ""
        }
      }, {
        "scope": [
          "support.type.exception",
          "constant.character",
          "constant.other"
        ],
        "settings": {
          "foreground": "#c4b798",
          "fontStyle": ""
        }
      }, {
        "scope": [
          "string",
          "markup.inline.raw"
        ],
        "settings": {
          "foreground": "#a8b78c",
          "fontStyle": ""
        }
      }, {
        "scope": [
          "meta.import",
          "meta.embedded",
          "punctuation.definition.arguments.begin.python",
          "punctuation.definition.arguments.end.python",
          "punctuation.definition.array.begin.json",
          "punctuation.definition.array.end.json",
          "punctuation.definition.dictionary.begin.json",
          "punctuation.definition.dictionary.end.json",
          "punctuation.definition.list.begin.python",
          "punctuation.definition.list.end.python",
          "punctuation.separator.arguments.python",
          "punctuation.separator.array.json",
          "punctuation.separator.dictionary.key-value.json",
          "punctuation.separator.dictionary.pair.json",
          "source"
        ],
        "settings": {
          "foreground": "#86908a",
          "fontStyle": ""
        }
      }, {
        "scope": [
          "keyword.control.from",
          "keyword.control.import",
          "keyword.operator.logical",
          "constant.language",
          "support.type"
        ],
        "settings": {
          "foreground": "#a6bab1",
          "fontStyle": ""
        }
      }, {
        "scope": "variable.parameter",
        "settings": {
          "foreground": "#ddb078",
          "fontStyle": ""
        }
      }, {
        "scope": "strong",
        "settings": {
          "foreground": "#5c4f55",
          "fontStyle": ""
        }
      }]
    }
  },
  "workbench.editorAssociations": [
    {
      "viewType": "jupyter-notebook",
      "filenamePattern": "*.ipynb"
    }
  ],
  "vsicons.dontShowNewVersionMessage": true,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "python.languageServer": "Pylance",
  "workbench.colorTheme": "Solarized Light"
}